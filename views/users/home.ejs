<!-- product_list start-->
<section class="product_list section_padding">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-12">
                <div class="section_tittle text-center">
                    <h2>awesome <span>shop</span></h2>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                
                <div class="product_list_slider owl-carousel">
                    <div class="single_product_list_slider">
                        <div class="row align-items-center justify-content-between">
                            <% let count = 0; %>
                            <% products.forEach((product, index) => { %>
                                <% if (count < 8) { %>
                                    <div class="col-lg-3 col-sm-6">
                                        <div class="single_product_item">
                                            <a href="/user/product/<%=product._id%>">

                                            <img src="<%= product.imageUrl[0] %>" alt="<%= product.name %>">
                                            <div class="single_product_text">
                                                <h4><%= product.name %></h4>
                                                <%if(product.discountPrice){%>
                                                    <h3 style="display: inline;">₹<%=product.discountPrice%></h3>
                                                    <h3 style="display: inline; text-decoration: line-through;margin-left: 65px;">₹<%=product.price%></h3>
                                                    <%}else{%>
                                                <h3>$<%= product.price %></h3>
                                                <%}%>
                                                <a href="/user/login" class="add_cart">+ add to cart<a href="#" class="like_us d-flex justify-content-center align-items-center">
                                                    <i class="bi bi-heart-fill"></i>
                                                </a></a>
                                            </div>
                                        </div>
                                    </div>
                                <% } else { %>
                                    <% if (count === 8) { %>
                                        </div> <!-- close the first row -->
                                    </div> <!-- close the first single_product_list_slider -->
                                    <div class="single_product_list_slider">
                                        <div class="row align-items-center justify-content-between">
                                    <% } %>
                                    <div class="col-lg-3 col-sm-6">
                                        <div class="single_product_item">
                                            <a href="/user/product/<%=product._id%>">
                                            <img src="<%= product.imageUrl[0] %>" alt="<%= product.name %>">
                                            <div class="single_product_text">
                                                <h4><%= product.name %></h4>
                                                <%if(product.discountPrice){%>
                                                    <h3 style="display: inline;">₹<%=product.discountPrice%></h3>
                                                    <h3 style="display: inline; text-decoration: line-through;margin-left: 65px;">₹<%=product.price%></h3>
                                                    <%}else{%>
                                                <h3>$<%= product.price %></h3>
                                                <%}%>
                                                <a href="/user/login" class="add_cart">+ add to cart<a href="#" class="like_us d-flex justify-content-center align-items-center">
                                                    <i class="bi bi-heart-fill"></i>
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                <% } %>
                                <% count++; %>
                            <% }) %>
                        </div>
                    </div>
                </div>
                
            </div>
        </div>
    </div>
</section>

